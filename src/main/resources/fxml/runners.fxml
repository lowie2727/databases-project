            fx:controller="be.uhasselt.databasesproject.controller.RunnerController">
    <children>
        <Text fx:id="runnerText" layoutX="14.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0"
              text="Runner Table" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0"/>
        <TableView fx:id="runnerTableView" layoutX="15.0" layoutY="30.0" prefHeight="310.0" prefWidth="950.0"
                   AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                   AnchorPane.topAnchor="30.0">
            <columns>
                <TableColumn fx:id="idTableColumn" prefWidth="75.0" text="id"/>
                <TableColumn fx:id="firstNameTableColumn" prefWidth="75.0" text="first name"/>
                <TableColumn fx:id="familyNameTableColumn" prefWidth="75.0" text="family name"/>
                <TableColumn fx:id="ageTableColumn" prefWidth="75.0" text="age"/>
                <TableColumn fx:id="weightTableColumn" prefWidth="75.0" text="weight"/>
                <TableColumn fx:id="lengthTableColumn" prefWidth="75.0" text="length"/>
                <TableColumn fx:id="streetNameTableColumn" prefWidth="75.0" text="street name"/>
                <TableColumn fx:id="houseNumberTableColumn" prefWidth="96.0" text="house number"/>
                <TableColumn fx:id="boxNumberTableColumn" prefWidth="64.0" text="box number"/>
                <TableColumn fx:id="postalCodeTableColumn" prefWidth="75.0" text="postal code"/>
